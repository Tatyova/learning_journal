    {
      "cell_type": "code",
      "source": [
        "import sympy as sy\n",
        "import numpy as np\n",
        "#3x+4y=5 -> 3x+4y-5=0\n",
        "x,y = sy.symbols('x y')\n",
        "f = 3*x + 4*y -5 \n",
        "f"
      ],
